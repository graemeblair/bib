@article{Snyder:2006,
	Abstract = {This article proposes a political economy of extraction framework that explains
political order and state collapse as alternative outcomes in the face of lootable
wealth. Different types of institutions of extraction can be built on lootable
resources---with divergent effects on political stability. If rulers are able to
forge institutions of extraction that give them control of revenues generated by
lootable resources, then these resources can contribute to political order by providing
the income with which to govern. Conversely, the breakdown or absence
of such institutions increases the risk of civil war by making it easier for rebels
to organize. The framework is used to explain two puzzling cases that experienced
sharply contrasting political trajectories in the face of lootable resources:
Sierra Leone and Burma. A focus on institutions of extraction provides a
stronger understanding of the wide range of political outcomes---from chaos,
to dictatorship, to democracy---in resource-rich countrie},
	Author = {Richard Snyder},
	Date-Added = {2010-11-29 19:47:53 -0500},
